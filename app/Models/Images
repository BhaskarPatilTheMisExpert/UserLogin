<?php

namespace App\Models;

use Illuminate\Database\Eloquent\Factories\HasFactory;
use Illuminate\Database\Eloquent\Model;


class Images extends Model
{
    use HasFactory;

    public $fillable = [
        'name'
    ];

      public function saveImg($input)
    {
        $query=$this->create($input);
        return $query;
    }
     
   

}
